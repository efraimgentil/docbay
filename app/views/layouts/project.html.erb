<!DOCTYPE html>
<!DOCTYPE html>
<html>
<head>
  <title>Docbay</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="wrapper">
    <!-- Sidebar -->
    <%= render "layouts/menu" %>
    <!-- /#sidebar-wrapper -->
    <!-- Page Content -->
    <div id="page-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <h1><%= @project.name %></h1>
                    
                    <ul>
                        <li><%= link_to "Notas", notes_path %></li>
                    </ul>
                        
                    <%= yield %>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>
<div>

</body>
</html>